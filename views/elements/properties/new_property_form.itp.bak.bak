<script type="text/javascript">
    var counter = 1;
    var draft_counter = 1;
    var time_unit = 'minute';
    var time_unit_suffix = '';
    $().ready(function() {
        // Auto save form at each 1 minute of time interval
        setInterval(auto_save_draft, 60000);
        $('form[name=addPropertyForm]').submit(function() {
            if (false == formValidationErrors)
                $(window).unbind('beforeunload.warning_msg');
        });
    });

    function auto_save_draft() {
        $.ajax({
            url: draft_url,
            type: 'post',
            data: $('form[name=addPropertyForm]').serialize(),
            success: function(response) {
                response = eval('(' + response + ')');
                if (response) {
                    if (counter > 59) {
                        time_unit = 'hour';
                        draft_counter = parseInt(counter / 60);
                    } else
                        draft_counter = counter;
                    if (draft_counter > 1)
                        time_unit_suffix = 's';
                    $('span#auto-save-status').text('Auto saved: ' + draft_counter + ' ' + time_unit + time_unit_suffix + ' ago');
                    $('input[name=auto_saved_id]').val(response.auto_saved_id);
                    counter++;
                }
            }
        });
    }

    // Warning on leaving this page
    $(window).unbind('beforeunload').bind('beforeunload.warning_msg', before_unload);
    function before_unload() {
        return "The changes you made will be lost if you navigate away from this page.";
    }


    $(function() {
        $('.date-pick').datePicker();
    });

    $(document).ready(function() {
        $('#first_select').change(function() {

            var select_value = escape($(this).val());

            $('#second_select').load('<?php echo WWW_ROOT; ?>/ajax.php?a=' + select_value);

        });
    });
</script>
<script language="JavaScript">

function showdiv(id) {
	var hide ="a"+ id;
	if (document.getElementById) { // DOM3 = IE5, NS6
		document.getElementById(id).style.display = 'block';
		document.getElementById(hide).style.display = 'none';
	}
	else {
		if (document.layers) { // Netscape 4
			document.id.display = 'block';
		}
		else { // IE 4
			document.all.id.style.display = 'block';
		}
	}
}
</script>
