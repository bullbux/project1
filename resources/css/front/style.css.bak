.marketing-slider{position: relative; height:355px;width:941px !important;padding:0 !important;margin-top: 25px; background: #fff;-moz-border-radius:3px 3px 3px 3px;-webkit-border-radius:3px 3px 3px 3px;border-radius:3px 3px 3px 3px;}
#slides .paging{width: 750px; float: right; padding-top:30px;}

.slides-count{color:#AEAFB3;display:inline-block;font-size:16px;font-weight:bold;margin-left:190px;}
.main_container #help-center, #home-search{
    background: #fff;
    -moz-border-radius:3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    margin-top:20px;
    padding:20px 30px 0;
    width:887px;
    color:#aeaeae;
}

#home-search{
    padding: 150px 30px;
}
#help-center .float-left{
    width: 270px;
    font-size:13px;
}

#help-center .float-right{
    width: 570px;
}

#help-center h3.title{
    color:#408AD2;
}

#help-center .faq-box{
    padding: 15px 0;
}

#help-center a{
    color:#aeaeae;
    text-decoration: none;
}

#help-center ul{
    list-style: none;
}

#help-center ol{
    padding-left: 20px;
    color:#aeaeae;
}

#help-center ol li div.answer{
    background:none repeat scroll 0 0 #F1F1F1;
    color:#666666;
    display:none;
    font-size:12px;
    line-height:19px;
    margin-top:8px;
    padding:10px;
}

#help-center li{
    padding-top: 5px;
}

#help-center input[type=text]{
    padding: 5px;
    color:#aeaeae;
    border: 1px solid #aeaeae;
    -moz-border-radius:3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    font-style:italic;
    width:180px;
}
#home-search{
    text-align: center;
	display: none;
}

#home-search input[type=text]{
    color:#aeaeae;
    border: 1px solid #aeaeae;
    -moz-border-radius:3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    font-size:16px;
    padding:7px;
    width: 615px;
}

#home-search input[type=submit]{
    padding:5px;
}

.home-search-box{
    padding-top: 20px;
}
