.narrow_select .select{#position:absolute; width:121px;	height:21px; border:none; padding:10px 32px 10px 10px; font: 12px arial,sans-serif;	background:url(../../images/input_bg.png) 0 0 no-repeat; overflow: hidden; left:0; top:15px;display:block;}

.feature_mid .narrow_select select {
width:157px !important; 
height:31px !important; 
margin-top:-40px !important;  
margin-bottom:20px;
display:block;
position:relative;
z-index:2;
}

.new_line {
border-top:1px solid #e1e1e1;
width:100%;
height:3px;
line-height:0px;
font-size:0px;
margin:13px 0 13px 0;
}

.neiglabel{#float:left; #padding:0 0 0px 6px; #width:150px; #position:relative;  color: #929497;
    font-weight: bold;}
	
#cityadvancedli{
font-family: Arial,Helvetica,sans-serif;
font-size: 12px; color: #929497;
}

.keyword{
font-family: Arial,Helvetica,sans-serif;
font-size: 12px; color: #929497;
}
.keyword input[type="text"] {
    font-style: italic;
    width: 300px; color: #BCBCBC;
	#background: none repeat scroll 0 0 transparent;
    #border: medium none;
    float: left;
    height: 32px;
    line-height: 32px;
    padding: 3px;
}
.keyword a.search {
    background: url("../../images/search_icon.png") no-repeat scroll 0 0 transparent;
    float: left;
    height: 26px;
    margin: 7px 1px 0 0;
    width: 28px;
}
#searchlabel{
font-family: Arial,Helvetica,sans-serif;
font-size: 12px; color: #F7A45C; font-weight: bold;
}

.field-error{
	color:#C21F1F;
	border:solid 1px #C21F1F;
}


/* new Sidebar - Advanced Search */
.new_advanced_search_sidebar{ background: #f5f5f5; border: 1px solid #ccc; border-radius: 5px; padding: 10px 10px; }
.new_advanced_search_sidebar .top_new_as_s{ background: #649bd3; color: #fff; border:  1px solid #ccc; padding: 10px 10px 3px; margin: 0 0 20px; position: relative; }
.new_advanced_search_sidebar .top_new_as_s .new_as_s_clearall{ display: block; color: #fff; text-decoration: none; margin: 0; }
.new_advanced_search_sidebar .top_new_as_s .new_as_s_title{ float: left; color: #000; text-decoration: none; font-size: 16px; font-weight: bold;; }
.new_advanced_search_sidebar .top_new_as_s .top_new_bottom_arrow{background: url(../../images/bottom_arrow.png) no-repeat; width: 22px; height: 12px; position: absolute; bottom: -11px; left: 50%; margin-left: 11px; }
.new_advanced_search_sidebar dl{ padding: 0 0 20px; clear: both; }
.new_advanced_search_sidebar dl dt{ font-size: 14px; font-weight: bold; color: #333; padding: 0 5px 5px; }
.new_advanced_search_sidebar dl dd ul{ background: #fff; padding: 3px; }
.new_advanced_search_sidebar h4{ padding: 10px 0 5px; }
.new_advanced_search_sidebar input[type=text]{ border: 1px solid #bbb; width: 170px; height: 16px; padding: 2px 5px; font-size: 12px; }
.new_advanced_search_sidebar input.normal_size{ width: 170px; }
.new_advanced_search_sidebar input.mini_size1{ width: 90px; }
.new_advanced_search_sidebar input.mini_size2{ width: 70px; }

.new_advanced_search_sidebar .search_history{ margin: 5px 0; list-style: none; }
.new_advanced_search_sidebar .search_history li{ padding: 2px 0; line-height: 12px;}
.new_advanced_search_sidebar .search_history .close_search_history{ display: block; float: left; width: 12px; height: 12px; line-height: 12px; text-align: center;font-size: 12px; background: #fff; color: #ff0000; margin-right: 5px; text-decoration: none; }


.new_advanced_search_sidebar select.normal_size{ width: 180px; height: 20px; }
.new_advanced_search_sidebar select.mini_size1{ width: 100px; height: 20px; }
.new_advanced_search_sidebar select.mini_size2{ width: 80px; height: 20px; }
.new_advanced_search_sidebar .more_features{ color: #669ad3; text-decoration: none; font-weight: bold; display: block; padding: 3px; }

.new_advanced_search_sidebar .check_features_bocks label{ float: none; weight: auto; }

.new_advanced_search_sidebar .features_list_ul{ float: none; list-style: none; }
.new_advanced_search_sidebar .features_list_ul li{ float: none; padding: 0; width: auto; }

.new_advanced_search_sidebar input.search_button{ width:123px; height:40px; background:url(../../images/search_btn.png) 0 0 no-repeat; display: block; border: none }
.new_advanced_search_sidebar input.search_button:hover{background-position:0 -40px;}

#sidebar_list_box{ padding: 5px 0 5px 5px; }
#sidebar_list_box .city_check{ float: none; }

.feature_mid .features_list_ul{ float: none; list-style: none; }
.feature_mid .features_list_ul li{ float: none; padding: 0; width: auto; text-align: left }

.check_features_bocks .features_list_ul{ float: none; list-style: none; }
.check_features_bocks .features_list_ul li{ float: none; padding: 0; width: auto; text-align: left }
.check_features_bocks .features_list_ul li label{ float: none; width: auto; }

.slogan{color: #676767;font-size: 36px;text-align: center;text-shadow:1px 2px 3px rgba(0,0,0,0.2);padding: 50px 0 14px;}
#greeting{text-align: center;color:#676767;font-weight: 600;font-size: 13px;padding-bottom: 35px;}
